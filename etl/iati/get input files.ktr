    <sql>DROP TABLE IF EXISTS `input_file`;
CREATE TABLE IF NOT EXISTS `input_file` (
  `input_file_key` int(11) NOT NULL AUTO_INCREMENT,
  `filename` varchar(255) DEFAULT NULL,
  `sfilename` varchar(255),
  `size` int(11) DEFAULT NULL,
  `typeFile` varchar(12) DEFAULT NULL,
  PRIMARY KEY (`input_file_key`)
) ENGINE=myisam DEFAULT CHARSET=utf8;

DROP TABLE IF EXISTS `input_file_error`;
CREATE TABLE IF NOT EXISTS `input_file_error` (
  `input_file_error_key` int(11) NOT NULL AUTO_INCREMENT,
  `filename` text,
  `lastmodifiedtime` datetime DEFAULT NULL,
  `size` int(11) DEFAULT NULL,
  `activities` varchar(10) DEFAULT NULL,
  `ValidationMsgFieldActivities` varchar(500) DEFAULT NULL,
  `organisation` varchar(10) DEFAULT NULL,
  `ValidationMsgFieldOrganisation` varchar(500) DEFAULT NULL,
  PRIMARY KEY (`input_file_error_key`)
) ENGINE=myisam  DEFAULT CHARSET=utf8 ;
</sql>
