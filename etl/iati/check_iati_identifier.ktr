    <sql>drop table if exists check_iati_identifier&#x3b;&#xa;create table  check_iati_identifier &#xa;&#x28;&#xa;  check_iati_identifier_key BIGINT AUTO_INCREMENT NOT NULL PRIMARY KEY&#xa;, &#x60;reporting-org-ref&#x60; VARCHAR&#x28;255&#x29;&#xa;, &#x60;iati-identifier&#x60; VARCHAR&#x28;255&#x29;&#xa;, &#x60;occurence&#x60; INT&#xa;, identifier_start_with_reporting_org INT default 0&#xa;, reporting_org_is_not_null INT default 0&#xa;, iati_identifier_is_not_null INT default 0&#xa;, valid_identifier INT default 0&#xa;&#x29; engine&#x3d;myisam  DEFAULT CHARSET&#x3d;utf8&#xa;&#x3b;&#xa;&#xa;&#xa;&#x23;occurence should be 1&#x3b;&#xa;insert into check_iati_identifier &#x28;&#x60;reporting-org-ref&#x60;,&#x60;iati-identifier&#x60;, occurence&#x29; &#xa;SELECT &#x60;reporting-org-ref&#x60;, &#x60;iati-identifier&#x60; , count&#x28;&#x2a;&#x29;&#xa;FROM &#x60;stage_activity&#x60;&#xa;GROUP BY &#x60;reporting-org-ref&#x60;, &#x60;iati-identifier&#x60;&#x3b;&#xa;&#xa;&#x23; no iati-identifier&#x3b;&#xa;update check_iati_identifier set iati_identifier_is_not_null&#x3d;1 &#xa;where &#x60;iati-identifier&#x60; is not null AND &#x60;iati-identifier&#x60; &#x3c;&#x3e; &#x22;no-iati-identifier&#x22;&#x3b;&#xa;&#xa;&#x23;no reporting-org-ref&#x3b;&#xa;update  check_iati_identifier set reporting_org_is_not_null&#x3d;1&#xa;where &#x60;reporting-org-ref&#x60; is not null&#x3b;&#xa;&#xa;&#x23;iati-identifier should start with reporting-org-ref and should not be the same as reporting-org-ref&#x3b;&#xa;update  check_iati_identifier set identifier_start_with_reporting_org&#x3d;1&#xa;where &#x60;iati-identifier&#x60; like concat&#x28; &#x60;reporting-org-ref&#x60; , &#x22;&#x25;&#x22; &#x29; &#xa; and &#x60;reporting-org-ref&#x60; is not null&#xa; and &#x60;reporting-org-ref&#x60; &#x3c;&#x3e; &#x60;iati-identifier&#x60;&#x3b;&#xa;&#xa;&#x23;valid iati-identifier if all tests are ok&#x3b;&#xa;update  check_iati_identifier set valid_identifier&#x3d;1&#xa;where&#xa;    &#x60;occurence&#x60; &#x3d;1&#xa;AND identifier_start_with_reporting_org&#x3d;1&#xa;AND reporting_org_is_not_null&#x3d;1&#xa;AND iati_identifier_is_not_null&#x3d;1&#xa;&#xa;</sql>
